generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model NumOfHolders {
  id        Int      @id
  date      DateTime
  contractAddress  String
  nHolders   Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
